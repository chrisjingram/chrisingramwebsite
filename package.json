{
  "name": "jekyll-webpack-base",
  "version": "1.0.0",
  "main": "src/js/main.js",
  "license": "MIT",
  "author": "MadeBySphere",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "autoprefixer": "^10.2.1",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.26.7",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.1.0",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.0.4",
    "material-design-icons": "^3.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --mode production && bundle exec jekyll build",
    "build-demo": "NODE_ENV=production webpack --mode production && bundle exec jekyll build --config _config.yml,_config.demo.yml",
    "netlify": "rm -rf dist/* && webpack && concurrently -k -n \"WEBPACK,JEKYLL\" -c \"bgBlue.bold,bgGreen.bold\" \"webpack -w\" \"bundle exec jekyll serve --config _config.yml,_config.local.yml\"",
    "start": "rm -rf dist/* && webpack && nf start"
  },
  "dependencies": {
    "aos": "^3.0.0-beta.6",
    "luxon": "^2.5.2",
    "node-fetch": "^2.6.0",
    "postcss": "^8.2.4",
    "postcss-loader": "^4.1.0",
    "smoothscroll-polyfill": "^0.4.4",
    "swiper": "^5.3.6",
    "tailwindcss": "^2.0.2"
  }
}
